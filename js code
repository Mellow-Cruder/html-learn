    <script>
        // Main Mega Menu Functions
        $(document).ready(function () {

            $('.mens ul li ul li ul').hover(function () {
                $(this).parent('li').addClass('active-menu');
            }, function () {
                $(this).parent('li').removeClass('active-menu');
            });

            $('.mens ul li ul li ul li ul').hover(function () {
                $(this).parent('li').addClass('active-menu-level2');
            }, function () {
                $(this).parent('li').removeClass('active-menu-level2');
            });

            $('.toggle-primary-nav').click(function () {
                $('.primary-nav ul').slideToggle();
            });

            // Off Canvas Menu
            $('.navbar-toggle').click(function () {
                $(this).toggleClass('menu-toggled');
            });


            $(window).resize(function () {
                if ($(window).width() <= 992) {
                    $('.mens ul li.has-child a').attr('href', '#0');
                    $(document).on('click', '.access_menu .dropdown-menu', function (e) {
                        e.stopPropagation();
                    });
                } else {
                    $('.mens ul li.has-child a').removeClass('hide');
                    $('.dropdown-toggle').dropdownHover().dropdown();
                    $(document).on('click', '.access_menu .dropdown-menu', function (e) {
                        e.stopPropagation()
                    });
                }
            }).resize();
        });
    </script>
    <script>
        $(window).load(function () {
            $('.menu-item-bordered#homemenu').addClass('active');
        });
    </script>
	
	
